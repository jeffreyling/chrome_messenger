{
  "manifest_version":  2,

  "name": "Messenger",
  "icons": { "128": "messenger_icon_128.png" },
  "description": "This extension embeds Facebook Messenger into Chrome!",
  "version": "1.0",

  "browser_action": {
    "default_icon": "messenger_icon.png",
    "default_popup": "popup.html",
    "default_title": "Messenger"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
